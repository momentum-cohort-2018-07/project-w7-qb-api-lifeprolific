<h1 class=''>Log In</h1>

<% if flash[:notice] %>
  <div class=""><%= flash[:notice] %></div>
<% end %>
<% if flash[:error_message] %>
  <div class=""><%= flash[:error_message] %></div>
<% end %>

<%= form_with(url: session_path, :html => {:class => ''}) do |form| %>

  <div class="">
    <%= form.label :username %>
    <div class="">
      <%= form.text_field :username, :class => "", :placeholder => '' %>
    </div>
  </div>

  <div class="">
    <%= form.label :password %>
    <%= form.password_field :password, class: '' %>
  </div>


  <%= submit_tag "Take off", :class => "" %>

<% end %>

